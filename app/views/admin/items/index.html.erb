<h2>商品一覧</h2>
<table>
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th colspan="3"></th>
      <th>ステータス</th>
    </tr>
  </thead>
  <today>
  <%= @items.each do |item| %>
    <div>
      <td><%= item.id %></td>
      <td><%= item.name %></td>
      <td><%= item.price %></td>
      <td><%= item.genre %></td>
      <td colspan="3"></td>
      <td><%= item.is_active %></td>
    </div>
  <%end%>
  </today>
</table>
